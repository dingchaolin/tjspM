    $(".btnDelete").click(function () {
        var c_id= $(this).parents('tr').attr('c-id');
        $.ajax({
            type: 'GET',
            url: '/other/versionUrlDelete',
            data: {id:c_id},
            success: function (result) {
                if (result.result == "ok") {
                    alert('删除成功!');
