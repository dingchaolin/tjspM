<table id="roomlist" class="table table-striped table-bordered table-hover bootstrap-datatable responsive">
    <thead>
    <tr>
        <th>用户昵称 </th>
        <th>所属房间</th>
        <th>用户Hash</th>
        <th>禁用IP</th>
        <th>操作管理员</th>
        <!--<th> 操  作 </th>-->
    </tr>
    </thead>
    <tbody >
    <% userList.forEach(function(user,index){%>
        <tr>
            <td class="col-md-1"><%= user.c_nickname %></td>
            <td class="col-md-1"><%if(!!user.c_chatroom_name){%><%=user.c_chatroom_name%><%}else{%>无<%}%></td>
            <td class="col-md-1"><%if(!!user.c_hash){%><%=user.c_hash%><%}else{%>无<%}%></td>
            <td class="col-md-1"><%= user.c_ip %></td>
            <td class="col-md-1"><%= user.c_admin %></td>
            <!--<td class="col-md-1">-->
            <!--<button class="btn btn-danger lock">-->
            <!--<i class="glyphicon glyphicon-trash icon-white"></i>-->
            <!--<span class="status">锁 定</span>-->
            <!--</button>-->
            <!--</td>-->
        </tr>
    <%})%>
    </tbody>
</table>
